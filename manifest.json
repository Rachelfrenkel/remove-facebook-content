{
  "manifest_version": 3,
  "name": "Remove Facebook Homepage Content",
  "version": "1.0",
  "description": "Hides all content on the Facebook homepage. Option to just remove video reels",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
